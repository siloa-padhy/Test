#include<iostream>
using namespace std;

int main( )
{
    char str[80];
    char str1[80];
     int k=0;
    cin.getline(str, 80);
     char c;
  while (str[k])
  {
    c=str[k];
    putchar (toupper(c));
    str1[k]=toupper(c);
    k++;
  }
	
   
    int l;     
    for(l = 0; str1[l] != '\0'; l++);

    int i;
    for(i = 0; (i < l/2) && (str1[i] == str1[l - i - 1]); i++);

    if(i == l/2)
        cout << "Palindrome";
    else
        cout << "Not a palindrome";
	
    return 0;
}
